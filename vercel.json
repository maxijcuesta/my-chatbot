{
  "version": 2,
  "builds": [
    {
      "src": "pages/api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/pages/api/chatbot.js"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/index.js"
    }
  ]
}