{
    "version": 2,
    "builds": [
      {
        "src": "pages/api/chatbot.js",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/chatbot"
      }
    ]
  }